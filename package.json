{
    "name": "ipage-element-expand",
    "version": "0.1.3",
    "keywords": [
        "ipage",
        "iform",
        "itable",
        "isearch"
    ],
    "description": "这是一个element-ui的扩展组件库，用于扩展element-ui的组件，使其更加强大。",
    "private": false,
    "author": {
        "name": "pcloth",
        "email": "pcloth@gmail.com"
    },
    "main": "src/index.js",
    "scripts": {
        "dev": "vue-cli-service serve",
        "build:docs": "vue-cli-service build",
        "build:lib": "vue-cli-service build --dest dist --target lib --name index src/index.js",
        "build:npm":"node build/webpack.lib.js",
        "lint": "vue-cli-service lint",
        "open": "dev serve -o docs"
    },
    "website": "https://lanyueos.com",
    "github": "https://github.com/pcloth/ipage-element-expand",
    "homepage": "https://github.com/pcloth/ipage-element-expand",
    "repository": {
        "type": "git",
        "url": "https://github.com/pcloth/ipage-element-expand"
    },
    "bugs": {
        "url": "https://github.com/pcloth/ipage-element-expand/issues"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^4.0.0",
        "@vue/cli-plugin-eslint": "^4.0.0",
        "@vue/cli-service": "^4.0.0",
        "babel-eslint": "^10.0.3",
        "babel-helper-vue-jsx-merge-props": "^2.0.3",
        "babel-plugin-syntax-jsx": "^6.18.0",
        "babel-plugin-transform-vue-jsx": "^3.7.0",
        "core-js": "^3.3.2",
        "element-ui": "^2.15.14",
        "eslint": "^5.16.0",
        "eslint-plugin-vue": "^5.0.0",
        "less": "^3.10.3",
        "less-loader": "^5.0.0",
        "markdown-it": "^8.4.1",
        "markdown-it-anchor": "^5.0.2",
        "markdown-it-chain": "^1.3.0",
        "markdown-it-container": "^2.0.0",
        "highlight.js": "^9.3.0",
        "node-sass": "4.14.1",
        "sass-loader": "9.0.3",
        "transliteration": "^1.1.11",
        "vue": "^2.6.10",
        "vue-dotmd-loader": "^0.2.0",
        "vue-router": "^3.1.5",
        "vue-template-compiler": "^2.6.10",
        "vuepress": "^1.9.10"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {
            "no-unused-vars": "warn"
        },
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "eslintIgnore": [
        "**/*.md"
    ],
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ],
    "dependencies": {
        "js-export-excel": "^1.1.4",
        "jszip": "^3.10.1"
    }
}
